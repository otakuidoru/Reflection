<?xml version="1.0" encoding="UTF-8"?>
<level>
	<!-- Initial Setup -->
	<setup>
		<!-- Emitters -->
		<emitters>
			<emitter id="1">
				<position x="0.5" y="0.1"></position>
				<direction>NORTH</direction>
				<colortype>RED</colortype>
				<active>true</active>
			</emitter>
		</emitters>

		<!-- Receptors -->
		<receptors>
			<receptor id="2">
				<position x="0.1" y="0.5"></position>
				<direction>EAST</direction>
				<colortype>RED</colortype>
			</receptor>
		</receptors>

		<!-- Mirrors -->
		<mirrors>
			<mirror id="3">
				<position x="0.5" y="0.5"></position>
				<direction>SOUTH</direction>
			</mirror>
		</mirrors>

		<!-- Bonus Stars -->
		<bonusstars>
			<bonusstar id="4">
				<position x="0.5" y="0.375"></position>
			</bonusstar>
			<bonusstar id="5">
				<position x="0.5" y="0.25"></position>
			</bonusstar>
			<bonusstar id="6">
				<position x="0.3" y="0.5"></position>
			</bonusstar>
		</bonusstars>
	</setup>

	<!-- Win Conditions -->
	<winconditions>
		<wincondition>
			<!-- Emitters -->
			<emitters>
				<emitter id="1" active="true"></emitter>
			</emitters>

			<!-- Mirrors -->
			<mirrors>
				<mirror id="3" direction="WEST"></mirror>
			</mirrors>
		</wincondition>
	</winconditions>
</level>

