cocos compile -p android -m debug && \
echo "Removing apk..." && \
rm -f /run/user/1000/gvfs/dav+sd:host=Spice%2520client%2520folder._webdav._tcp.local/Shared/ZenProgramming/Reflection-debug.apk && \
echo "Copying apk..." && \
cp ./bin/debug/android/Reflection-debug.apk /run/user/1000/gvfs/dav+sd:host=Spice%2520client%2520folder._webdav._tcp.local/Shared/ZenProgramming/Reflection-debug.apk

